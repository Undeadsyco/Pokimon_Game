{
  "plugins": [
    "plugins/markdown",
    "jsdoc-mermaid",
    "node_modules/better-docs/typescript",
    "node_modules/better-docs/category",
    "node_modules/better-docs/typedef-import"
  ],
  "recurseDepth": 10,
  "source": {
    "include": [
      "src"
    ],
    "exclude": [
      "node_modules"
    ],
    "includePattern": ".+\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "template": "node_modules/better-docs",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "search": true,
    "default": {
      "staticFiles": {
        "include": [
          "./docs-src/statics"
        ]
      }
    }
  }
}